<h1> Edit a market: </h1>


<% if @market.errors.any? %>
  <ul>
    <% @market.errors.each do |column, message| %>
      <li>
        <%= column.capitalize %> <%= message %>
      </li>
    </ul>
    <% end %>
<% end %>

<%= form_for @market do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :address %>
  <%= f.text_field :address %>
  <%= f.label :city %>
  <%= f.text_field :city %>
  <%= f.label :county %>
  <%= f.text_field :county %>
  <%= f.label :state %>
  <%= f.text_field :state %>
  <%= f.label :zip %>
  <%= f.text_field :zip %>
  <%= f.submit %>
<% end %>
