<%= form_for @market do |t| %>
  <%= t.label :name %>
  <%= t.text_field :name %>

  <%= t.label :address %>
  <%= t.text_field :address %>

  <%= t.label :city %>
  <%= t.text_field :city %>

  <%= t.label :county %>
  <%= t.text_field :county %>

  <%= t.label :state %>
  <%= t.text_field :state %>

  <%= t.label :zip %>
  <%= t.text_field :zip %>

  <%= t.submit %>
<% end %>
