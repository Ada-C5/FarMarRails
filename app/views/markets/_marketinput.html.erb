<section>
  <h3>Update Your Market</h3>
  <%= form_for @market do |t| %>
    <article>
      <p>
      <%= t.label :name %>
      <%= t.text_field :name %>
      </p>

      <p>
        <%= t.label :address %>
        <%= t.text_field :address %>
      </p>

      <p>
        <%= t.label :city %>
        <%= t.text_field :city %>
      </p>

      <p>
        <%= t.label :county %>
        <%= t.text_field :county %>
      </p>

      <p>
        <%= t.label :state %>
        <%= t.text_field :state %>
      </p>

      <p>
        <%= t.label :zip %>
        <%= t.text_field :zip %>
      </p>

      <%= t.submit %>
    </article>
  <% end %>
</section>
